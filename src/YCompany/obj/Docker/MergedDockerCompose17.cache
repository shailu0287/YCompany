services:
  claimdb:
    container_name: claimdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: SwN12345678
    image: mcr.microsoft.com/mssql/server:2017-latest
    ports:
    - published: 1434
      target: 1434
    restart: always
  claims.api:
    build:
      context: C:\Users\shailen203\source\repos\YCompany\src\YCompany
      dockerfile: Services/Claims/Claims.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Claims.API
      target: base
    container_name: claim.api
    depends_on:
      claimdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionStrings:ClaimConnectionString: Server=claimdb;Database=ClaimDb;User
        Id=sa;Password=SwN12345678
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventBusSettings:HostAddress: amqp://guest:guest@rabbitmq:5672
      NUGET_FALLBACK_PACKAGES: ''
    image: claimsapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Claims.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8000
      target: 80
    tty: true
    volumes:
    - C:\Users\shailen203\source\repos\YCompany\src\YCompany\Services\Claims\Claims.API:/app:rw
    - C:\Users\shailen203\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\shailen203\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\shailen203\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\shailen203\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\shailen203\source\repos\YCompany\src\YCompany:/src:rw
  rabbitmq:
    container_name: rabbitmq
    image: rabbitmq:3-management-alpine
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    restart: always
version: '3.4'